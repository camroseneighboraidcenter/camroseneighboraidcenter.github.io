{
  "private": true,
  "type": "module",
  "name": "camroseneighboraidcenter.ca",
  "description": "Camrose Neighbor Aid Center website",
  "version": "0.0.1",
  "scripts": {
    "lint": "prettier --check . && find . -type f -name '*.html' ! -path '*/node_modules/*' ! -path './dist/*' ! -path './.astro/*' -exec html-validate {} + && stylelint \"**/*.css\" --report-needless-disables --report-invalid-scope-disables && eslint .",
    "fix": "prettier --write . && eslint --fix . && stylelint \"**/*.css\" --fix --report-needless-disables --report-invalid-scope-disables",
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.13",
    "astro": "^5.16.6",
    "remark-attr": "^0.11.1",
    "sharp": "^0.34.5",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/hast": "^3.0.4",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "html-validate": "^10.5.0",
    "markdownlint": "^0.40.0",
    "markdownlint-cli": "^0.47.0",
    "prettier": "^3.7.4",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  }
}
